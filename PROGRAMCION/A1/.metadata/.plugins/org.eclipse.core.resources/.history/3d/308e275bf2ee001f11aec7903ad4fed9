package com.model;

public class Triangulo {

	
	private double lado1;
	private double lado2;
	private double lado3;
	private double[] cantLados = {this.lado1,this.lado2,this.lado3};

	
	public Triangulo( double lado1, double lado2, double lado3) {
		super();
		this.lado1 = lado1;
		this.lado2 = lado2;
		this.lado3 = lado3;
		}
	

	

	
	
	
}
